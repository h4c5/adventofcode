   "source": [
    "import os\n",
    "from pathlib import Path\n",
    "\n",
    "home = os.getenv(\"HOME\", \"\")\n",
    "\n",
    "TOKEN_FILE = Path(home) / \".config/aocd/token\"\n",
    "TOKEN_FILE.parent.mkdir(parents=True, exist_ok=True)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Save a new token"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "TOKEN = \"\"\n",
    "\n",
    "with TOKEN_FILE.open(\"w\") as f:\n",
    "    f.write(TOKEN)"
   ]
  }
